{
  "name": "https-express",
  "version": "1.0.0",
  "author": "Sam Pastoriza [pastorsj](https://github.com/pastorsj)",
  "repository": "https://github.com/pastorsj/https-express",
  "private": false,
  "scripts": {
    "start": "babel-node ./src/server.js",
    "https": "babel-node ./src/server.js --type=https",
    "http": "babel-node ./src/server.js --type=http",
    "build": "webpack --env build --display-error-details",
    "dev": "webpack --progress --colors --env dev --display-error-details",
    "lint": "eslint src/*.js"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "body-parser": "~1.17.2",
    "chai": "^4.1.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~3.0.1",
    "express": "~4.15.4",
    "express-session": "^1.15.5",
    "helmet": "^3.8.1",
    "method-override": "^2.3.9",
    "mocha": "^3.5.0",
    "morgan": "~1.8.2",
    "readline": "^1.3.0",
    "serve-favicon": "~2.4.3",
    "sinon": "^3.2.1",
    "spdy": "^3.4.7",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^3.2.2",
    "eslint-config-rose": "^1.0.0",
    "eslint-config-xo": "^0.15.3",
    "eslint-loader": "^1.7.1",
    "webpack": "^3.5.6",
    "webpack-node-externals": "^1.6.0"
  }
